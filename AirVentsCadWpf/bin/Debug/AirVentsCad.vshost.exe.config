<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
     <section name="AirVentsCadWpf.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <connectionStrings>
    <add name="AirVentsCadWpf.Properties.Settings.WorkingSqlConnection"
      connectionString="SWPlusDB" />
    <add name="AirVentsCadWpf.Properties.Settings.TestSqlConnection"
      connectionString="DB-Tes" />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <userSettings>
    <AirVentsCadWpf.Properties.Settings>
      <setting name="UserName" serializeAs="String">
        <value />
      </setting>
      <setting name="Password" serializeAs="String">
        <value />
      </setting>
      <setting name="ConnectionToSQL" serializeAs="String">
        <value>Data Source=srvkb;Initial Catalog=SWPlusDB;Persist Security Info=True;User ID=sa;Password=PDMadmin;MultipleActiveResultSets=True</value>
      </setting>
      <setting name="SourceFolder" serializeAs="String">
        <value>D:\Vents-PDM</value>
      </setting>
      <setting name="DestinationFolder" serializeAs="String">
        <value>D:\Vents-PDM</value>
      </setting>
      <setting name="Domain" serializeAs="String">
        <value>vents.local</value>
      </setting>
      <setting name="PdmBaseName" serializeAs="String">
        <value>Vents-PDM</value>
      </setting>
      <setting name="TestPdmBaseName" serializeAs="String">
        <value>Vents-PDM</value>
      </setting>
      <setting name="Developer" serializeAs="String">
        <value>False</value>
      </setting>
    </AirVentsCadWpf.Properties.Settings>
  </userSettings>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ServiceReference2.IBomTableService">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
        <behavior name="ServiceReference1.IBomTableService">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_I1cService" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxBufferSize="1000000000" maxReceivedMessageSize="1000000000"/>
        <binding name="BasicHttpBinding_IEpdmService" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxBufferSize="1000000000" maxReceivedMessageSize="1000000000"/>
        <binding name="BasicHttpBinding_ITaskService" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxBufferSize="1000000000" maxReceivedMessageSize="1000000000"/>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://srvkb:8080/VentsService/FirstContract"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_I1cService"
        contract="ServiceReference6.I1cService" name="BasicHttpBinding_I1cService" />
      <endpoint address="http://srvkb:8080/VentsService/SecondContract"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IEpdmService"
        contract="ServiceReference6.IEpdmService" name="BasicHttpBinding_IEpdmService" />
      <endpoint address="http://srvkb:8080/VentsService/ThirdContract"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITaskService"
        contract="ServiceReference6.ITaskService" name="BasicHttpBinding_ITaskService" />
    </client>
  </system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NLog" publicKeyToken="5120e14c03d0593c" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.0.0" newVersion="3.1.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>

